{
  "name": "show-link-list",
  "displayName": "Show Link List",
  "publisher": "Avengineers",
  "description": "Shows a Dialog with a pair of labels and links configured in settings.json, e.g. links to support channels.",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:show-link-list.show"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/avengineers/show-link-list"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "show-link-list.show",
        "title": "Show Links",
        "category": "Support"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.54.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}