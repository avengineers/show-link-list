{
  "name": "show-link-list",
  "displayName": "Favorite URLs",
  "publisher": "Avengineers",
  "description": "Shows a Dialog with a pair of labels and URLs as a QuickPick list configured in settings.json, e.g. URLs to support channels. Those can be the favorite URLs or anything project relevant to documentation or anything else.",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "favorite",
    "favorites",
    "link",
    "links",
    "url",
    "urls",
    "configurable",
    "list",
    "url list",
    "url favorites"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onCommand:favorite-urls.show"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/avengineers/show-link-list"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "favorite-urls.show",
        "title": "URLs",
        "category": "Favorites"
      }
    ],
    "configuration": {
      "title": "favorite-urls Configuration",
      "properties": {
        "favorite-urls": {
          "list": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "label": {
                  "type": "string"
                },
                "url": {
                  "type": "string"
                }
              }
            }
          },
          "sort": {
            "type": "boolean"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0"
  },
  "dependencies": {
    "@vscode/vsce": "^2.16.0"
  }
}
